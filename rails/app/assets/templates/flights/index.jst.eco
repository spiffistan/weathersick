<% for flight in @flights: %>
<div class="flight search-result card flip">
  <div class="front face"> 

    <a href="<%= flight.booking_link %>">
    <div class="price">
        <div class="from">from</div>
        <div class="fare">
          $<%= flight.total_fare %>
        </div>
        <div class="way">one-way</div>
    </div>
    </a>
  </div>
</div>
<% end %>
